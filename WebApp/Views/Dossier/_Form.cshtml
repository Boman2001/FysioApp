@model WebApp.Dtos.Dossier.CreateDossierDto

    <div class="form-group">
                <label asp-for="PatientId" class="control-label"></label>
                <select class="form-control" asp-for="PatientId" asp-items="Model.Patients">
                    <option onclick="OpenPatientDialog()">Add new patient</option>
                </select>
                <span asp-validation-for="PatientId" class="text-danger"></span>
            </div>

            <div class="form-group">
                <label asp-for="IntakeById" class="control-label"></label>
                <select class="form-control" asp-for="IntakeById" asp-items="Model.Staff">
                </select>
                <span asp-validation-for="IntakeById" class="text-danger"></span>
            </div>

            <div class="form-group">
                <label asp-for="SupervisedById" class="control-label"></label>
                <select class="form-control" asp-for="SupervisedById" asp-items="Model.Staff">
                    <option value="" selected></option>
                </select>
                <span asp-validation-for="SupervisedById" class="text-danger"></span>
            </div>

            <div class="form-group">
                <label asp-for="HeadPracticionerId" class="control-label"></label>
                <select class="form-control" asp-for="HeadPracticionerId" asp-items="Model.Staff">
                </select>
                <span asp-validation-for="HeadPracticionerId" class="text-danger"></span>
            </div>

            <div class="form-group">
                <label asp-for="DiagnoseCodeId" class="control-label"></label>
                <select class="form-control" asp-for="DiagnoseCodeId" asp-items="Model.Diagnoses">
                </select>
                <span asp-validation-for="DiagnoseCodeId" class="text-danger"></span>
            </div>

            <div class="form-group">
                <label asp-for="Description" class="control-label"></label>
                <textarea asp-for="Description" class="form-control"/> </textarea>
                <span asp-validation-for="Description" class="text-danger"></span>
            </div>

            <div class="form-group">
                <label asp-for="IsStudent" class="control-label"></label>
                <select class="form-control" asp-for="IsStudent">
                    <option value="true">Yes</option>
                    <option value="false">No</option>
                </select>
                <span asp-validation-for="IsStudent" class="text-danger"></span>
            </div>

            <div class="form-group">
                <label asp-for="AdmissionDate" class="control-label"></label>
                <input type="date" class="form-control" asp-for="AdmissionDate" id="dateInput">
                <span asp-validation-for="AdmissionDate" class="text-danger"></span>
            </div>